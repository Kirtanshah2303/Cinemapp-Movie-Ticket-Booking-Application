    <uses-permission android:name="android.permission.INTERNET" />

<!--    Add permission to excess internal storage to store pdf-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/applogo1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".App">
        <activity android:name=".ConfirmationActivity"></activity>
        <activity android:name=".PaymentActivity" />
        <activity android:name=".SeatBookingActivity" />
        <activity android:name=".forgot_password" />
        <activity android:name=".demothirdActivity" />
        <activity android:name=".AboutUs_activity" />
        <activity android:name=".SplashscreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".demoaAtication" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>